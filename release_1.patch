+  .post(
+    `/olab/maps/${mapId}/nodes/${edgeData.source}/links`,
+    createEdgeBody(edgeData.target),
+  )
+  .then(({ data: { data: { id } } }) => id)
+  .catch((error) => {
+    throw error;
+  });
