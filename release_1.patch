+      const graph = cloneDeep([state.nodes, state.edges]);
+      const { id } = action;
+
+      graph.forEach((items) => {
+        items.forEach((item) => {
+          if (id) {
+            item.isSelected = item.data.id === id;
+          } else {
+            item.isSelected = false;
+          }
+        });
+      });
+
+      const [nodes, edges] = graph;
+
+      return {
+        ...state,
+        nodes,
+        edges,
+      };
