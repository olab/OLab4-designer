+      label, color, variant, thickness, isHidden,
+    } = this.state;
+    const {
+      x, y, isDragging, connectDragSource, connectDragPreview, layoutEngineType, classes,
+    } = this.props;
+
+    if (isDragging) {
+      return null;
+    }
+
+    return (
+      <ModalWrapper
+        x={x}
+        y={y}
+        ref={instance => connectDragPreview(instance)}
+      >
