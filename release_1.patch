+          <article>
+            <ColorPicker
+              label="Color"
+              color={color}
+              onChange={this.handleColorChange}
+            />
+          </article>
+        </LinkEditorBody>
+        <LinkEditorFooter>
+          <ActionButton onClick={this.applyChanges}>
+            Save
+          </ActionButton>
+        </LinkEditorFooter>
+      </ModalWrapper>
+    );
+  }
