+
+import type { IOutlinedInputProps } from './types';
+
+import styles from './styles';
+
+const OutlinedInput = ({
+  name,
+  label,
+  classes,
+  value,
+  placeholder = '',
+  onChange,
+  fullWidth = false,
+}: IOutlinedInputProps) => (
+  <TextField
+    type="text"
+    name={name}
+    label={label}
+    value={value}
+    placeholder={placeholder}
+    onChange={onChange}
