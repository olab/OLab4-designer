+
+      return {
+        ...state,
+        nodes,
+      };
+    }
+    case EXCHANGE_NODE_ID: {
+      const { nodes, edges } = action;
+
+      return {
+        ...state,
+        nodes,
+        edges,
+      };
+    }
+    case EXCHANGE_EDGE_ID: {
+      const { edges } = action;
+
+      return {
+        ...state,
+        edges,
+      };
