+              label="Hidden"
+              labelPlacement="start"
+              checked={isHidden}
+              onChange={this.handleSwitchChange}
+            />
+            {layoutEngineType !== 'VerticalTree' && !this.isLinkHasSibling && (
+              <ChangeDirectionWrapper>
+                <InputLabel>Change Direction</InputLabel>
+                <IconButton
