+
+const undoRedoMiddleware = store => next => (action) => {
+  if ([
+    CREATE_NODE,
+    UPDATE_NODE,
+    DELETE_NODE,
+    CREATE_NODE_WITH_EDGE,
+    CREATE_EDGE,
+    UPDATE_EDGE,
+    DELETE_EDGE,
+  ].includes(action.type)) {
+    store.dispatch({ type: SAVE_MAP_TO_UNDO });
+  }
