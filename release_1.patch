+// @flow
+import {
+  type ScopedObjectsActions,
+  type ScopedObjectsState as ScopedObjectsType,
+  SCOPED_OBJECTS_FAILED,
+  SCOPED_OBJECTS_SUCCEEDED,
+  SCOPED_OBJECTS_REQUESTED,
+} from './types';
