+const spec = {
+  beginDrag: props => props,
+  endDrag: (props, monitor, component) => {
+    if (!monitor.didDrop()) {
+      return;
+    }
+
+    const dropResult = monitor.getDropResult();
+
+    if (!dropResult) {
+      return;
+    }
+
+    const { x: offsetX, y: offsetY } = dropResult;
+    const x = offsetX + props.x;
+    const y = offsetY + props.y;
