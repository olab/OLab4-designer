+      classes: {
+        input: classes.input,
+      },
+    }}
+    InputLabelProps={{
+      classes: {
+        root: classes.focusedLabel,
+      },
+    }}
+    fullWidth={fullWidth}
+  />
+);
+
+export default withStyles(styles)(OutlinedInput);
