+              value={label}
+              onChange={this.handleInputChange}
+              fullWidth
+            />
+          </article>
+          <article>
+            <OutlinedSelect
+              label="Style"
+              name="variant"
+              labelWidth={40}
+              value={LINK_VARIANTS[variant - 1]}
