+  connectDragPreview: conn.dragPreview(),
+  isDragging: monitor.isDragging(),
+});
+
+export default connect(
+  mapStateToProps,
+  mapDispatchToProps,
+)(
+  withStyles(styles)(
+    DragSource(
+      DndContexts.VIEWPORT,
+      spec,
+      collect,
+    )(LinkEditor),
+  ),
+);
