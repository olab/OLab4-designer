+import {
+  CREATE_NODE,
+  UPDATE_NODE,
+  DELETE_NODE,
+  CREATE_EDGE,
+  CREATE_NODE_WITH_EDGE,
+  UPDATE_EDGE,
+  DELETE_EDGE,
+  SAVE_MAP_TO_UNDO,
