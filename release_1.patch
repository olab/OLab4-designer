+    const {
+      isLinkSource,
+      data: {
+        isCollapsed, isLocked, width: currentWidth, height, type, title, text, color,
+      },
+    } = this.props;
+
+    const currentHeight = isCollapsed ? COLLAPSED_HEIGHT : height;
+
+    return (
+      <foreignObject
+        x={-currentWidth / 2}
+        y={-currentHeight / 2}
+        width={currentWidth}
+        height={currentHeight}
+        viewBox={`0 0 ${currentWidth} ${currentHeight}`}
+      >
+        <NodeComponent
+          isLinked={isLinkSource}
+          isLocked={isLocked}
+          isCollapsed={isCollapsed}
+          resizeRef={this.resizeRef}
+          width={currentWidth}
+          height={height}
+          type={type}
+          title={title}
+          text={text}
+          color={color}
+        />
+      </foreignObject>
+    );
+  }
+
+  render() {
+    const { x, y } = this.state;
+    const { id } = this.props;
+
+    return (
+      <g
+        id={id}
+        ref={this.nodeRef}
+        transform={`translate(${x}, ${y})`}
+      >
+        {this.renderShape()}
+      </g>
+    );
+  }
